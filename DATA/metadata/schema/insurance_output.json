{
    "entity_type": "INSURANCE",
    "version": "1.0",
    "description": "Schema định nghĩa các cột đầu ra cho thực thể INSURANCE (Bảo hiểm)",
    "columns": {
        "symbol": {
            "type": "string",
            "required": true,
            "description": "Mã chứng khoán"
        },
        "report_date": {
            "type": "string",
            "format": "date",
            "required": true,
            "description": "Ngày báo cáo (YYYY-MM-DD)"
        },
        "year": {
            "type": "integer",
            "required": true,
            "description": "Năm tài chính"
        },
        "quarter": {
            "type": "integer",
            "required": true,
            "description": "Quý tài chính"
        },
        "net_profit": {
            "type": "float",
            "required": true,
            "description": "Lợi nhuận ròng (Tỷ VND)"
        },
        "total_revenue": {
            "type": "float",
            "required": true,
            "description": "Tổng doanh thu phí bảo hiểm (Tỷ VND)"
        },
        "combined_ratio": {
            "type": "float",
            "required": false,
            "description": "Tỷ lệ kết hợp (%)"
        },
        "loss_ratio": {
            "type": "float",
            "required": false,
            "description": "Tỷ lệ bồi thường (%)"
        },
        "expense_ratio": {
            "type": "float",
            "required": false,
            "description": "Tỷ lệ chi phí (%)"
        },
        "investment_yield": {
            "type": "float",
            "required": false,
            "description": "Tỷ suất sinh lời đầu tư (%)"
        },
        "solvency_ratio": {
            "type": "float",
            "required": false,
            "description": "Tỷ lệ khả năng thanh toán (%)"
        },
        "roe": {
            "type": "float",
            "required": true,
            "description": "Tỷ suất sinh lời trên vốn chủ sở hữu (%)"
        },
        "roa": {
            "type": "float",
            "required": true,
            "description": "Tỷ suất sinh lời trên tổng tài sản (%)"
        }
    }
}