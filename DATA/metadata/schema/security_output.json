{
    "entity_type": "SECURITY",
    "version": "1.0",
    "description": "Schema định nghĩa các cột đầu ra cho thực thể SECURITY (Chứng khoán)",
    "columns": {
        "symbol": {
            "type": "string",
            "required": true,
            "description": "Mã chứng khoán"
        },
        "report_date": {
            "type": "string",
            "format": "date",
            "required": true,
            "description": "Ngày báo cáo (YYYY-MM-DD)"
        },
        "year": {
            "type": "integer",
            "required": true,
            "description": "Năm tài chính"
        },
        "quarter": {
            "type": "integer",
            "required": true,
            "description": "Quý tài chính"
        },
        "net_profit": {
            "type": "float",
            "required": true,
            "description": "Lợi nhuận ròng (Tỷ VND)"
        },
        "total_revenue": {
            "type": "float",
            "required": true,
            "description": "Tổng doanh thu hoạt động (Tỷ VND)"
        },
        "brokerage_ratio": {
            "type": "float",
            "required": false,
            "description": "Tỷ trọng doanh thu môi giới (%)"
        },
        "margin_lending_ratio": {
            "type": "float",
            "required": false,
            "description": "Tỷ trọng doanh thu cho vay ký quỹ (%)"
        },
        "prop_trading_ratio": {
            "type": "float",
            "required": false,
            "description": "Tỷ trọng doanh thu tự doanh (%)"
        },
        "cost_income": {
            "type": "float",
            "required": false,
            "description": "Tỷ lệ Chi phí / Thu nhập (%)"
        },
        "roe": {
            "type": "float",
            "required": true,
            "description": "Tỷ suất sinh lời trên vốn chủ sở hữu (%)"
        },
        "roa": {
            "type": "float",
            "required": true,
            "description": "Tỷ suất sinh lời trên tổng tài sản (%)"
        },
        "capital_ratio": {
            "type": "float",
            "required": false,
            "description": "Tỷ lệ an toàn vốn (%)"
        }
    }
}